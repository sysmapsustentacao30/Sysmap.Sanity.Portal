@model Sysmap.Portal.Sanity.Models.User

@{
    ViewData["Title"] = "DeleteUser";
}

<h3>Tem certeza que deseja deletar este usuario?</h3>
<hr />
<div>
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.Email)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Email)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.TypeUser)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.TypeUser)
        </dd>
    </dl>

    <form asp-action="DeleteUser">
        <input asp-for="idUser" class="form-control" type="hidden" readonly />
        <input type="submit" value="Deletar" class="btn btn-sm btn-outline-danger" /> |
        <a asp-action="ListUser"><i class="fas fa-angle-double-left"></i> Voltar a Lista</a>
    </form>
</div>
